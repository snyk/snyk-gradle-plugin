{
  "name": "snyk-gradle-plugin",
  "description": "Snyk CLI Gradle plugin",
  "homepage": "https://github.com/snyk/snyk-gradle-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-gradle-plugin"
  },
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "lib/init.gradle"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project tsconfig.json --format stylish",
    "prepare": "npm run build",
    "test": "npm run lint && npm run test-functional && npm run test-system",
    "test-functional": "tap -R spec ./test/functional/*.test.[tj]s",
    "test-system": "tap -R spec --timeout=180 ./test/system/*.test.[tj]s"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^4.9.1",
    "@types/sinon": "^7.0.10",
    "@types/tmp": "0.0.34",
    "sinon": "^2.4.1",
    "tap": "^12.6.1",
    "tslint": "^5.14.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "clone-deep": "^0.3.0",
    "tmp": "0.0.33",
    "tslib": "^1.9.3"
  }
}
