{
  "name": "snyk-gradle-plugin",
  "description": "Snyk CLI Gradle plugin",
  "homepage": "https://github.com/snyk/snyk-gradle-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-gradle-plugin"
  },
  "main": "dist/lib/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "lib/init.gradle"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project tsconfig.json --format stylish",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "test": "npm run build && npm run lint && npm run test-functional && npm run test-system",
    "test-functional": "tap test/functional/*.test.* -Rspec --timeout=180 --node-path ts-node --test-file-pattern '/\\.[tj]s$/'",
    "test-system": "tap test/system/*.test.* -Rspec --timeout=180 --node-path ts-node --test-file-pattern '/\\.[tj]s$/'"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^4.9.1",
    "@types/sinon": "^7.0.10",
    "@types/tmp": "0.0.34",
    "semantic-release": "^15",
    "sinon": "^2.4.1",
    "tap": "github:snyk/node-tap#alternative-runtimes",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "clone-deep": "^0.3.0",
    "tmp": "0.0.33",
    "ts-node": "8.0.3",
    "tslib": "^1.9.3"
  }
}
